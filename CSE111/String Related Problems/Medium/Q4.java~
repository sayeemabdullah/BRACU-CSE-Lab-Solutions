import java.util.*;
public class Q4{
public static void main (String args[]){
  Scanner input = new Scanner(System.in);
  String str = input.next();
  String output;
  for(int i=0;i<str.length();i++){
    for(int j=0;j<i;j++){
      if(str.charAt(i)==str.charAt(j)){
      continue;
      }
      else{
      output+=str.charAt(i);
      }
  }
}
}
}