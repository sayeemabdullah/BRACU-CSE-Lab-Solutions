import java.util.*;
public class Q8{
public static void main (String args[]){
  Scanner input = new Scanner(System.in);
  int[] arr = new int[11];
  for(int i=0;i<arr.length;i++){
      int number=input.nextInt();
      arr[number]++;
  }
  for(int i=0;i<=9;i++){
    if(arr[number]>2 && arr[number]<5){
    System.out.println(i+" "+arr[i]);
   }
}
}
}